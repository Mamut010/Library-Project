* {
    margin: 0;
    padding: 0;
    box-sizing: border-box;
}

#footer
{
    background-color: #431a20;
    position: fixed;
    left: 0;
    bottom: 0;
    text-align: center;
    width: 100%;
}

body
{
    overflow-x: auto;
}

.background
{
    background-color:black;
}

h1
{
    font-family:serif;
    font-size: 56px;
    color: white;
    text-align: center;
    float: left;
    margin-left: 20%;
    margin-top: 5%;
}

.clearfixadmin a:link, .clearfixadmin a:visited
{
    width: 50%;
    border: 1px black;
    /*background-color: black;*/
    /*color: white;*/
    padding: 14px 2px;
    margin: auto;
    font-size: 16px;
    font-family: serif;
    text-align: center;
    text-decoration: none;
    display: flex;
    justify-content: center;
    align-items: center;
}
.clearfixadmin a:hover
{
    background-color: white;
    color: black;
    text-decoration: underline;
}
spamadmin
{
    display: block;
}
.clearfixadmin
{
    content: "";
    clear: both;
    overflow: auto;
}

.menu
{
    background-color: black;
    color: white;
}
